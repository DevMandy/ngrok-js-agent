{
    "enabled": true,
    "inbound": [
      {
        "expressions": null,
        "name": "Add JWT authentication and rate limiting",
        "actions": [
          {
            "type": "rate-limit",
            "config": {
              "name": "Only allow 30 requests per minute",
              "algorithm": "sliding_window",
              "capacity": 30,
              "rate": "60s",
              "bucket_key": [
                "req.Headers['\\''x-api-key'\\'']"
              ]
            }
          }
        ]
      }
    ],
    "outbound": null
  }
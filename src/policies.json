{
  "inbound": [
    {
      "name": "Rate Limiting",
      "expressions": null,
      "actions": [
        {
          "type": "rate-limit",
          "config": {
            "name": "Only allow 30 requests per minute",
            "algorithm": "sliding_window",
            "capacity": 3,
            "rate": "60s",
            "bucket_key": [
              "req.Headers['x-api-key']"
            ]
          }
        }
      ]
    }
  ]
}